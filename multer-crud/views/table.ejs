        <div id="page-wrapper">
        <div class="col-md-12 graphs">
	   <div class="xs">
  	 <h3 id="h3">Student Details</h3>
  	<div class="bs-example4" data-example-id="contextual-table">
    
        <%record.map((val,index)=>{%>
            <table border="1" id="dtbl">
                 <tr id="ttr">
                     <thead>
                     <th id="tth" scope="col">Id</th>
                     <th id="ttd"><%=index+1%></th>                   
                     </thead>          
                 </tr>
                 <br> <br>
                 <tr id="ttr">
                    <thead>
                         <th  id="tth" scope="col">Student Image</th>
                         <td  id="ttd"><img src="<%=val.img%>" height="100px" width="100px" alt=""></td>
                    </thead>            
                 </tr>

                 <tr id="ttr">
                    <thead>
                    <th id="tth" scope="col">Student Id</th>
                    <td id="ttd"><%=val.sid%></td>  
                    </thead>          
                </tr>
         
                 <tr id="ttr">
                     <thead>
                     <th id="tth" scope="col">Entery Year</th>
                     <td id="ttd"><%=val.year%></td>  
                     </thead>          
                 </tr>
         
                 <tr id="ttr">
                     <thead>
                     <th id="tth" scope="col">Student Name</th>
                     <td id="ttd"><%=val.name%></td>  
                     </thead>          
                 </tr>
                 <tr id="ttr">
                     <thead>
                     <th id="tth" scope="col">Birth Of Date</th>
                     <td id="ttd"><%=val.bod%></td>  
                     </thead>          
                 </tr>
                 <tr id="ttr">
                     <thead>
                     <th id="tth" scope="col">Gender</th>
                     <td id="ttd"><%=val.gender%></td>  
                     </thead>          
                 </tr>
         
                 <tr id="ttr">
                     <thead>
                     <th id="tth" scope="col">Address</th>
                     <td id="ttd"><%=val.address%></td>  
                     </thead>          
                 </tr>
         
                 <tr id="ttr">
                     <thead>
                     <th id="tth" scope="col">Student E-mail</th>
                     <td id="ttd"><%=val.email%></td>  
                     </thead>          
                 </tr>
         
                 <tr id="ttr">
                     <thead>
                     <th id="tth" scope="col">Phone Number</th>
                     <td id="ttd"><%=val.number%></td>  
                     </thead>          
                 </tr>
                 <tr id="ttr">
                     <thead>
                     <th id="tth" scope="col">School / Collage Name</th>
                     <td id="ttd"><%=val.school%></td>  
                     </thead>          
                 </tr>
                 <tr id="ttr">
                     <thead>
                     <th id="tth" scope="col"></th>
                     <td id="ttd"><button type="button" class="btn btn-outline-success"><a href="/editdata?id=<%=val.id %>">Edit</a></button>
                         <button type="button" class="btn btn-outline-primary">
                             <a href="/deletedata?id=<%=val.id %>">Delete</a>
                             </button>
                     </td>    
                     </thead>          
                 </tr>
                   
            </table>
            <%})%>
            

